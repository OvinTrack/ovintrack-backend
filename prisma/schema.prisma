generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

// Exemple de mod√®le - vous pouvez le modifier selon vos besoins
model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Sheep {
  id        Int      @id @default(autoincrement())
  lat       Float
  lng       Float
  updatedAt DateTime @default(now())
}
